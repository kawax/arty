{
    "name": "revolution/arty",
    "description": "artisan only application",
    "keywords": [
        "arty"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "charlottedunois/yasmin": "^0.5.1",
        "illuminate/notifications": "5.7.*",
        "illuminate/view": "5.7.*",
        "laravel-notification-channels/discord": "^1.0",
        "laravel-zero/framework": "5.7.*",
        "ramsey/uuid": "^3.8",
        "restcord/restcord": "^0.3.0",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["arty"]
}
